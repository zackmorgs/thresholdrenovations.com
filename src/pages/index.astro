---
import CommonHead from "../components/common-head.astro";
import "../styles/css/main.min.css";

import Nav from "../components/nav.astro";
import HeroMain from "../components/hero-main.astro";
import Services from "../components/services.astro";
import Testimonials from "../components/testimonials.astro";
import ReviewUs from "../components/review-us.astro";
import Cta from "./../components/cta.astro";
import Footer from "../components/footer.astro";
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Threshold Renovations | Home Remodeling in Winona, MN</title>
		<meta
			name="title"
			content="Threshold Renovations | Home Remodeling in Winona, MN"
		/>
		<meta
			name="description"
			content="Threshold Renovations specializes in expert home remodeling, kitchens, baths, and custom renovations in Winona, MN. Quality craftsmanship you can trust."
		/>

		<CommonHead />
		<script type="application/ld+json">
			{
				"@context": "https://schema.org",
				"@type": "WebSite",
				"name": "Threshold Renovations",
				"url": "https://thresholdrenovations.com",
				"description": "Threshold Renovations specializes in expert home remodeling, kitchens, baths, and custom renovations in Winona, MN. Quality craftsmanship you can trust."
			}
		</script>
	</head>
	<body>
		<Nav />
		<HeroMain />
		<main>
			<Services />
			<Testimonials />
			<ReviewUs />
			<Cta />
		</main>
		<Footer />
	</body>
</html>
