---
import CommonHead from "../../../components/common-head.astro";
import "../../../styles/css/main.min.css";

import Nav from "../../../components/nav.astro";
import CustomWorkServices from "../../../components/custom-work-services.astro";
import Footer from "../../../components/footer.astro";
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>
			Outbuildings & Specialty Structures Winona, MN | Threshold
			Renovations
		</title>
		<meta
			name="title"
			content="Outbuildings & Specialty Structures Winona, MN | Threshold Renovations"
		/>
		<meta
			name="description"
			content="Threshold Renovations builds custom outbuildings and specialty structures in Winona, MN — from sheds and garages to animal dwellings and more."
		/>
		<CommonHead />
		<script type="application/ld+json">
			{
				"@context": "https://schema.org",
				"@type": "Service",
				"name": "Custom Work",
				"description": "Threshold Renovations builds custom outbuildings and specialty structures in Winona, MN — from sheds and garages to animal dwellings and more.",
				"provider": {
					"@type": "LocalBusiness",
					"name": "Threshold Renovations",
					"url": "https://thresholdrenovations.com",
					"address": {
						"@type": "PostalAddress",
						"addressLocality": "Winona",
						"addressRegion": "MN",
						"addressCountry": "US"
					}
				},
				"areaServed": "Winona, MN",
				"url": "https://thresholdrenovations.com/services/custom-work"
			}
		</script>
	</head>
	<body>
		<Nav />
		<main>
			<header class="hero" id="hero_outbuildings">
				<h2>Custom Work</h2>
			</header>
			<CustomWorkServices />
			<section class="section">
				<div class="content">
					<div class="content-text"></div>
				</div>
			</section>
		</main>
		<Footer />
	</body>
</html>
