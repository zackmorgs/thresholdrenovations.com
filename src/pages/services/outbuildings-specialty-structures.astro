---
import CommonHead from "../../components/common-head.astro";
import "../../styles/css/main.min.css";

import Nav from "../../components/nav.astro";
import Footer from "../../components/footer.astro";
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>
			Outbuildings & Specialty Structures Winona, MN | Threshold
			Renovations
		</title>
		<meta
			name="title"
			content="Outbuildings & Specialty Structures Winona, MN | Threshold Renovations"
		/>
		<meta
			name="description"
			content="Threshold Renovations builds custom outbuildings and specialty structures in Winona, MN — from sheds and garages to animal dwellings and more."
		/>
		<CommonHead />
		<script type="application/ld+json">
			{
				"@context": "https://schema.org",
				"@type": "Service",
				"name": "Outbuildings & Specialty Structures",
				"description": "Threshold Renovations builds custom outbuildings and specialty structures in Winona, MN — from sheds and garages to animal dwellings and more.",
				"provider": {
					"@type": "LocalBusiness",
					"name": "Threshold Renovations",
					"url": "https://thresholdrenovations.com",
					"address": {
						"@type": "PostalAddress",
						"addressLocality": "Winona",
						"addressRegion": "MN",
						"addressCountry": "US"
					}
				},
				"areaServed": "Winona, MN",
				"url": "https://thresholdrenovations.com/services/outbuildings-specialty-structures"
			}
		</script>
	</head>
	<body>
		<Nav />
		<main>
			<header class="hero" id="hero_outbuildings">
				<h2>Outbuildings & Specialty Structures</h2>
			</header>
			<section class="section">
				<div class="content">
					<div class="content-text">
						<p>
							Need extra space? We design and build custom sheds,
							garden workshops, chicken coops, and other specialty
							structures that are as practical as they are
							durable.
						</p>
						<div class="picture">
							<img
								src="/assets/img/services/animal-dwellings/34.jpg"
								alt="Famous Winona Bird House"
							/>
							<div class="caption">
								Famous Winona Bird House
							</div>
						</div>
						<br>
						<div class="picture">
							<img
								src="/assets/img/services/animal-dwellings/3.jpg"
								alt="Custom Chicken Coop"
							/>
							<div class="caption">
								Custom Chicken Coop
							</div>
						</div>
						<br>
						<div class="picture">
							<img
								src="/assets/img/services/animal-dwellings/6.jpg"
								alt="Custom Animal Dwelling"
							/>
							<div class="caption">
								Custom Animal Dwelling
							</div>
						</div>

					</div>
				</div>
			</section>
		</main>
		<Footer />
	</body>
</html>
